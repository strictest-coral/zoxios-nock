{
  "name": "zoxios-nock",
  "version": "0.0.0",
  "description": "Nock creation using zoxios definitions",
  "engines": {
    "node": ">=14.17"
  },
  "main": "dist/index.js",
  "files": [
    "/dist"
  ],
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "build": "tsup ./src/index.ts --format cjs,esm --dts",
    "lint": "eslint src",
    "release": "npm run build && standard-version",
    "prepare": "husky install",
    "prepublish": "npm run build"
  },
  "keywords": [
    "zod",
    "axios",
    "zoxios",
    "http",
    "request",
    "tests",
    "nock"
  ],
  "repository": {
    "url": "https://github.com/strictest-coral/zoxios-nock"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "author": "Almog Vagman Ciprut",
  "license": "MIT",
  "eslintConfig": {
    "extends": "strictest-coral",
    "rules": {
      "max-lines-per-function": "off"
    },
    "ignorePatterns": [
      "**/*.type-spec.ts"
    ]
  },
  "jest": {
    "verbose": true,
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "tsup": {
    "minify": true
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@types/jest": "^29.5.2",
    "eslint": "^8.43.0",
    "eslint-config-strictest-coral": "^0.0.6",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.0",
    "tsup": "^7.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "axios": "^1.4.0",
    "zod": "^3.21.4",
    "zoxios": "^0.1.4"
  },
  "dependencies": {
    "@anatine/zod-mock": "^3.12.1",
    "nock": "^13.3.1"
  }
}
